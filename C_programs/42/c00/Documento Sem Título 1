#include <unistd.h>

void	putchar(char );
void	ft_print_comb(void);

int	main(void)
{
	return 0;
}

void	putchar(char c)
{
	write(1, &c, 1);
}

void	ft_print_comb(void)
{
	char a;
	char b;
	char c;

	a = '0';
	while (a <= '9')
	{
		b = a + 1;
		while (b <= '8')
		{
			c = b + 1;
			while (c <= '7')
			{
				putchar(a);
				putchar(b);
     			putchar(c);
				putchar('\n');		
			}
		}
	}
}
